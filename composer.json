{
	"name": "flarum-lang/spanish-mexico",
	"type": "flarum-extension",
	"description": "Spanish (Mexico) language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/spanish-mexico",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"es_MX-locale",
		"spanish-mexico-locale",
		"spanish-mexico"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/spanish-mexico/issues",
		"source": "https://github.com/flarum-lang/spanish-mexico"
	},
	"require": {
		"flarum/core": "^1.7"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Spanish (Mexico)",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "es_MX",
			"title": "Spanish (Mexico)"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
